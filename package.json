{
  "name": "seisplotjs",
  "version": "1.0.0",
  "description": "Javascript library for parsing, manipulating and displaying seismic data.",
  "main": "lib/index.js",
  "dependencies": {
    "seisplotjs-distaz": ">= 1.0.0",
    "seisplotjs-fdsnevent": ">= 1.0.1",
    "seisplotjs-fdsnstation": ">= 1.0.0",
    "seisplotjs-fdsndataselect": ">= 1.0.0",
    "seisplotjs-filter": ">= 1.0.0",
    "seisplotjs-miniseed": ">= 1.1.0",
    "seisplotjs-model": ">= 1.0.0",
    "seisplotjs-seedcodec": ">= 1.0.1",
    "seisplotjs-seedlink": ">= 1.0.0",
    "seisplotjs-traveltime": ">= 1.0.0",
    "seisplotjs-waveformplot": ">= 1.2.0"
  },
  "scripts": {
    "lint": "eslint 'src/*.js' ",
    "examplelint": "eslint -c .eslintrc_examples.js 'example/serviceCheck/serviceCheck.js' 'example/serviceCheck/allServiceTests.js' 'example/socal/socal.js' 'example/southcarolina/southcarolina.js' ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "babel --presets es2015 -d lib/ src/",
    "standalone": "browserify src/index.js --standalone seisplotjs -o seisplotjs_100_standalone.js -t [ babelify --presets [ es2015 ] ]",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crotwell/seisplotjs.git"
  },
  "keywords": [
    "seismology",
    "miniseed",
    "steim1",
    "steim2"
  ],
  "author": "Philip Crotwell <crotwell@seis.sc.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crotwell/seisplotjs/issues"
  },
  "homepage": "https://github.com/crotwell/seisplotjs",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "qunit": "^0.9.1"
  }
}
