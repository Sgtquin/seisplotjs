{
  "name": "seisplotjs",
  "version": "1.2.0-alpha.1",
  "description": "Javascript library for parsing, manipulating and displaying seismic data.",
  "main": "lib/index.js",
  "dependencies": {
    "seisplotjs-distaz": "^1.0.1",
    "seisplotjs-fdsndataselect": "^1.1.3-alpha.1",
    "seisplotjs-fdsnevent": "^1.1.1-alpha.1",
    "seisplotjs-fdsnstation": "^1.1.1-alpha.1",
    "seisplotjs-filter": "^1.1.0",
    "seisplotjs-miniseed": "^1.2.0",
    "seisplotjs-model": "^1.2.0-alpha.1",
    "seisplotjs-seedcodec": "^1.0.2",
    "seisplotjs-seedlink": "^1.1.0",
    "seisplotjs-traveltime": "^1.1.0",
    "seisplotjs-waveformplot": "^1.2.3"
  },
  "scripts": {
    "lint": "eslint 'src/*.js' ",
    "examplelint": "eslint -c .eslintrc_examples.js 'example/serviceCheck/serviceCheck.js' 'example/serviceCheck/allServiceTests.js' 'example/socal/socal.js' 'example/southcarolina/southcarolina.js' ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "babel --presets es2015 -d lib/ src/",
    "standalone": "browserify src/index.js --standalone seisplotjs -o seisplotjs_${npm_package_version}_standalone.js -t [ babelify ]",
    "prepare": "npm run compile",
    "version": "npm run compile && npm run standalone",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crotwell/seisplotjs.git"
  },
  "keywords": [
    "seismology",
    "miniseed",
    "steim1",
    "steim2"
  ],
  "author": "Philip Crotwell <crotwell@seis.sc.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crotwell/seisplotjs/issues"
  },
  "homepage": "https://github.com/crotwell/seisplotjs",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "qunit": "^1.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ],
            "node": "current"
          }
        }
      ]
    ]
  }
}
