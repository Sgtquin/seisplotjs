{
  "name": "seisplotjs",
  "version": "0.0.1",
  "description": "Javascript library for parsing, manipulating and displaying seismic data.",
  "main": "lib/index.js",
  "dependencies": {
    "seisplotjs-distaz": ">= 1.0.0",
    "seisplotjs-fdsnevent": ">= 0.0.1",
    "seisplotjs-fdsnstation": ">= 0.0.1",
    "seisplotjs-fdsndataselect": ">= 0.0.1",
    "seisplotjs-miniseed": ">= 1.0.0",
    "seisplotjs-seedcodec": ">= 1.0.0",
    "seisplotjs-seedlink": ">= 0.0.1",
    "seisplotjs-traveltime": ">= 0.0.1",
    "seisplotjs-waveformplot": ">= 1.0.0"
  },
  "scripts": {
    "lint": "eslint 'src/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "babel --presets es2015 -d lib/ src/",
    "standalone": "browserify src/index.js --standalone seisplotjs -o seisplotjs_001s_standalone.js -t [ babelify --presets [ es2015 ] ]",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crotwell/seisplotjs.git"
  },
  "keywords": [
    "seismology",
    "miniseed",
